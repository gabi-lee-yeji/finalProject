<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="spring.project.mapper.AdminMapper">
	
	<insert id="addCerti">
		insert into certiinfo values (
			<if test="category.equals('국가기술')">'N'+cert_seq.NEXTVAL,</if>
			<if test="category.equals('공인민간')">'P'+cert_seq.NEXTVAL,</if>
			<if test="category.equals('어학')">'L'+cert_seq.NEXTVAL,</if>
			#{cname},
			#{category},
			#{subCategory},
			#{cround},
			#{regStart},
			#{regEnd},
			#{reg_addStart},
			#{reg_addEnd},
			#{regStartTime},
			#{regEndTime},
			#{testDate},
			#{resDate}
		)
	</insert>
	
	
</mapper>